{
  "name": "petals",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.7.1",
    "@material-ui/icons": "^3.0.1",
    "parcel-bundler": "^1.11.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-form": "^8.1.0",
    "redux-saga": "^1.0.2"
  },
  "scripts": {
    "start": "NODE_ENV=development parcel serve -p 3000 public/index.html",
    "build": "NODE_ENV=production parcel build --no-source-maps public/index.html",
    "qa-build": "rm -rf dist & NODE_ENV=development parcel build public/index.html",
    "prebuild": "rm -rf dist",
    "test": "yarn clean && jest",
    "tslint": "tslint -c ./tslint.json --format stylish --project ./tsconfig.json 'src/**/*.{ts,tsx}'",
    "prettier": "prettier --write ./{src,__tests__}/**/*.{ts,tsx}",
    "clean": "rm -rf .cache & rm -rf dist"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "printWidth": 120,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@types/enzyme": "^3.9.0",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.9",
    "@types/material-ui": "^0.21.5",
    "@types/node": "^13.1.7",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^7.0.1",
    "@types/react-router-dom": "^4.3.1",
    "@types/redux": "^3.6.0",
    "@types/redux-form": "^7.4.15",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "jest": "^24.1.0",
    "jest-enzyme": "^7.0.1",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "prettier": "^1.15.3",
    "ts-jest": "^24.0.0",
    "tslint": "^5.13.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3333"
  }
}
